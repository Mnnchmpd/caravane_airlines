<%= form_for(@product) do |f| %>
<thead>
   <th>Name</th>
   <th>Description</th>
   <th>Price</th>
   <th>Address</th>
  <%= f.text_field :name, placeholder: 'Name' %>
  <%= f.text_field :description, placeholder: 'Description' %>
  <%= f.text_field :price, placeholder: 'Price' %>
  <%= f.text_field :address, placeholder: 'Address' %>
  <%= f.submit %>
   </thead>
<% end %>
