<div class="container">
<h1>Welcome to your dashboard !
  <p>Your Caravans:</p>
  <div class="cards">
    <% current_user.products.each do |product| %>
      <div class="container-b">
        <div class="card-category" role="img" style="width: 100%; height: 100%">
          <%= cl_image_tag product.photo, height: 300, width: 350, crop: :fill %>
          <p class="card-title">
            <%= link_to product_path(product) do %>
              <p><%= "#{product.name}"%></p>
              <p><%= "#{product.description}" %> <br>
              <%= "#{product.price}â‚¬" %> </br></p>
            <% end %>
          </p>
          </h1>
        </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
