<div class="container">
  <div class="form-wrapper" style="width: 400px; margin: 0 auto;">
    <h2> Book your caravan 👇🏻 </h2>
    <h4>

       <div class="col-md-8">


          <%= simple_form_for :search, method: "get" do |f| %>
            <%= f.input :debut, as: :string, placeholder: 'debut', required: false, input_html: {class: "datepicker"} %>
            <%= f.input :fin, as: :string, required: false, placeholder: 'fin', input_html: {class: "datepicker"} %>
            <div>
            <%= f.button :submit, "Book this caravan", class: "btn btn-dark btn-lg" %>
            <p><%= link_to "Back to all Caravan", root_path %></p>
          <% end %>
      </h4>
  </div>
</div>

